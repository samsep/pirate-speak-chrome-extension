{
  "manifest_version": 2,
  "name": "PirateSpeak",
  "version": "0.1",
  "description": "Translate your webpages from English to PirateSpeak!",
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "pirates/pirates.html",
    "background-page": "pirates/background.html"
  },
  "content_scripts": [
    {
      "matches": ['<all_urls>'],
      "css": ["pirates.css"],
      "js": ["pirates.js", "speak.js"]
    }
  ],
}
